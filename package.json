{
  "name": "mirage",
  "version": "1.1.0",
  "scripts": {
    "ng": "ng",
    "docker": "docker.sh",
    "apk:move": "cp ./platforms/android/app/build/outputs/apk/release/app-release-unsigned.apk ./apk",
    "apk:sign": "/usr/lib/Android/build-tools/29.0.3/apksigner sign --out apk/mirage.apk --ks apk/keystore.jks --ks-pass pass:MaxTri2012 --key-pass pass:MaxTri2012 apk/app-release-unsigned.apk",
    "apk:delete-old": "touch ./apk/mirage.apk && rm ./apk/mirage.apk",
    "apk:delete-unsigned": "touch ./apk/app-release-unsigned.apk && rm ./apk/app-release-unsigned.apk",
    "android:delete-local-properties": "touch ./platforms/android/local.properties && rm ./platforms/android/local.properties",
    "ng:serve": "ng serve",
    "ng:dev": "ng build -c cordova-dev",
    "ng:prod": "ng build -c cordova-prod",
    "cordova:dev": "cordova run android",
    "cordova:prod": "cordova build android --release --prod",
    "script:get": "touch game.script.js && rm game.script.js && wget -O game.script.js https://proxyconnection.touch.dofus.com/build/script.js",
    "script:get:beautified": "npm run script:get && js-beautify -r game.script.js",
    "start": "npm run android:delete-local-properties && npm run cordova:dev",
    "build": "npm run ng:dev",
    "release": "npm run ng:prod && npm run android:delete-local-properties && npm run apk:delete-old && npm run cordova:prod && npm run apk:move && npm run apk:sign && npm run apk:delete-unsigned",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "9.0.0",
    "@angular/cdk": "9.0.0",
    "@angular/common": "9.0.0",
    "@angular/compiler": "9.0.0",
    "@angular/core": "9.0.0",
    "@angular/fire": "^5.4.2",
    "@angular/flex-layout": "^9.0.0-beta.29",
    "@angular/forms": "9.0.0",
    "@angular/material": "^9.0.0",
    "@angular/platform-browser": "9.0.0",
    "@angular/platform-browser-dynamic": "9.0.0",
    "@angular/router": "9.0.0",
    "@datorama/akita": "^4.19.2",
    "cordova-android": "^8.1.0",
    "cordova-plugin-browsersync": "^1.1.0",
    "cordova-plugin-browsersync-gen2": "^1.1.7",
    "cordova-plugin-file": "^6.0.2",
    "cordova-plugin-indexeddb": "^0.1.2",
    "cordova-plugin-screen-orientation": "^3.0.2",
    "cordova-sqlite-storage": "^4.0.0",
    "es6-promise-plugin": "^4.2.2",
    "firebase": "^7.8.1",
    "rxjs": "6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.900.1",
    "@angular/cli": "9.0.1",
